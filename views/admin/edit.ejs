<!DOCTYPE html>
<html>
<head>
	<title>userlist</title>
</head>
<body>
	<h1>Edit user</h1>

<a href="/admin/category">Category</a> |
<a href="/admin/request">Request</a> |
<a href="/admin/userlist">User</a> |


	<a href="/home">Back</a> |
	<a href="/logout">logout</a>

<br><br>

<form method="post">
	<table border="0">

					<% for(var i=0; i< user.length; i++){ %>

		<tr>
			<td>Username</td>
			<td><input type="text" name="username" value="<%=user[i].username%>"></td>
		</tr>
		<tr>
			<td>PASSWORD</td>
			<td><input type="password" name="password" value="<%=user[i].password%>"></td>
		</tr>

		<tr>
			<td>Admin</td>
			<td>Yes<input type="radio" name="is_admin" value="1">No<input type="radio" name="is_admin" value="0"></td>
		</tr> 
		<tr>
			<td></td>
			<td><input type="submit" name="submit" value="Save"></td>
		</tr>
					<% } %>

	</table>
</form>
</body>
</html>